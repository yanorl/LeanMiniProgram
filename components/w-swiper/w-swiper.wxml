<!--components/w-swiper/w-swiper.wxml-->
<swiper class="swiper" circular autoplay interval="3000" indicator-dots indicator-color="#ffffff" indicator-active-color="#ff5777">
  <block wx:for="{{images}}" wx:key="{{index}}">
    <swiper-item class="swiper-item">
      <image src="{{item}}" mode="widthFix" lazy-load/>
    </swiper-item>
  </block>
</swiper>